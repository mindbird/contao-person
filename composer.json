{
  "name": "mindbird/contao-person",
  "description": "Simple list of people bundled in archives",
  "minimum-stability": "stable",
  "license": "proprietary",
  "keywords":["contao", "person", "personen", "katalog", "ansprechpartner", "kontakt", "contact"],
  "type":"contao-bundle",
  "authors": [
    {
      "name": "mindbird",
      "email": "info@mindbird.de",
      "homepage": "http://www.mindbird.de",
      "role":"Developer"
    }
  ],
  "require": {
    "contao/core-bundle": "~5.6"
  },
  "require-dev": {
    "contao/manager-plugin": "^2.0"
  },
  "conflict": {
    "contao/manager-plugin": "<2.0 || >=3.0"
  },
  "extra": {
    "contao-manager-plugin": "Mindbird\\Contao\\Person\\ContaoManager\\Plugin"
  },
  "autoload": {
    "psr-4": {
      "Mindbird\\Contao\\Person\\": "src/"
    }
  },
  "config": {
    "allow-plugins": {
      "contao-components/installer": true,
      "php-http/discovery": true,
      "contao/manager-plugin": true
    }
  }
}
